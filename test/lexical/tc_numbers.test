source sf_numbers.jolt

assert_eq      dec_1      1234567890
assert_eq      dec_2      1345678901
assert_eq      dec_3      456
assert_eq      dec_4      98765432
assert_eq      dec_5      12893

assert_eq      hex_1      256
assert_eq      hex_2      511
assert_eq      hex_3      268435456
assert_eq      hex_4      28036591
assert_eq      hex_5      28036576

assert_eq      oct_1      64
assert_eq      oct_2      32768
assert_eq      oct_3      127
assert_eq      oct_4      8

assert_eq      bin_1      9
assert_eq      bin_2      3
assert_eq      bin_3      256
assert_eq      bin_4      15

assert_eq      flt_1      10.1250
assert_eq      flt_2      0.0400
assert_eq      flt_3      5630.0000
assert_eq      flt_4      0.1250
assert_eq      flt_5      87.0000
assert_eq      flt_6      3800.0000

assert_eq      hft_1      10.1250
assert_eq      hft_2      0.2500
assert_eq      hft_3      203.2500
assert_eq      hft_4      36.6250
assert_eq      hft_5      87.0000
assert_eq      hft_6      224.0000
